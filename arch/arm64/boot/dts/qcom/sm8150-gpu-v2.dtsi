&soc {
	gpu_opp_table_v2: gpu_opp_table_v2 {
		compatible = "operating-points-v2";
		
		opp-695000000 {
			opp-hz = /bits/ 64 <695000000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_NOM_L2>;
		};

		opp-560000000 {
			opp-hz = /bits/ 64 <560000000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_NOM_L1>;
		};

		opp-427000000 {
			opp-hz = /bits/ 64 <427000000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_NOM>;
		};

		opp-315000000 {
			opp-hz = /bits/ 64 <315000000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_NOM>;
		};

		opp-225000000 {
			opp-hz = /bits/ 64 <225000000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_MIN_SVS>;
		};

		opp-135000000 {
			opp-hz = /bits/ 64 <135000000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_MIN_SVS>;
		};
	};
};
